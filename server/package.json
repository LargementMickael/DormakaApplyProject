{
  "name": "dormakaba_server",
  "version": "1.0",
  "description": "Dormakaba back-end",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc --build tsconfig.json",
    "preserve": "npm run build",
    "serve": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"nodemon -L dist/index.js\"",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "mongodb": "^4.2.1",
    "rimraf": "^2.7.1",
    "ts-node": "^9.1.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "4.16",
    "@types/node": "^16.11.11",
    "concurrently": "^6.4.0",
    "nodemon": "^2.0.15",
    "ts-node-dev": "^1.1.8",
    "tslint": "^5.12.1",
    "typescript": "^3.9.10"
  }
}
